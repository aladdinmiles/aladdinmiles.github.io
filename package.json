{
  "name": "aladdinmiles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "cp .env.example .env && BUILD_PROFILE=development next dev",
    "build": "BUILD_PROFILE=production next build",
    "start": "BUILD_PROFILE=production next start",
    "lint": "next lint",
    "deploy": "BUILD_PROFILE=static next build && touch out/.nojekyll && git add out/ && git commit -m \"Deploy\" && git subtree push --prefix out origin gh-pages"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^2.2.0",
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@types/validator": "^13.9.0",
    "autoprefixer": "10.4.14",
    "axios": "^1.7.8",
    "date-fns": "^4.1.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "formik": "^2.4.6",
    "jws": "^4.0.0",
    "next": "13.4.12",
    "phone": "^3.1.37",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.3.5",
    "react-icons": "^5.3.0",
    "react-qr-code": "^2.0.11",
    "react-svg": "^16.1.19",
    "react-toastify": "^9.1.3",
    "react-web-share": "^2.0.2",
    "sharp": "^0.32.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "validator": "^13.11.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/jws": "^3.2.10",
    "husky": "^8.0.3",
    "prettier": "^3.0.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,css,md}": "prettier --write"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "proxy": "https://ambe.aladdinmiles.com"
}
